<html lang="en">
<%- include("./partials/head.ejs") %>

<body>
  <%- include("./partials/nav.ejs") %>

  <div class="blogs content">
    <h2>All dependencies</h2>

    <% if (deps.length > 0) { %>
      <% deps.forEach(dep => { %>
        <a href="<%= dep.link %> " class="sinlge">
          <h3 class="title"><%= dep.title %></h3>
          <p class="snippet"><%= dep.link %></p>
        </a>

      <% }) %>
    <% } else { %>
      <p>There are no dependencies to display...</p>
    <% } %>

  </div>

  <%- include("./partials/footer.ejs") %>
</body>
</html>